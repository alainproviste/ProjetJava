<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Index</servlet-name>
  	<display-name>Index</display-name>
  	<description></description>
  	<servlet-class>blog.servlets.Index</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddArticle</servlet-name>
  	<display-name>AddArticle</display-name>
  	<description></description>
  	<servlet-class>blog.servlets.AddArticle</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddAuteur</servlet-name>
  	<display-name>AddAuteur</display-name>
  	<description></description>
  	<servlet-class>blog.servlets.AddAuteur</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListeArticle</servlet-name>
  	<display-name>ListeArticle</display-name>
  	<description></description>
  	<servlet-class>blog.servlets.ListeArticle</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListeAuteur</servlet-name>
  	<display-name>ListeAuteur</display-name>
  	<description></description>
  	<servlet-class>blog.servlets.ListeAuteur</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Article</servlet-name>
  	<display-name>Article</display-name>
  	<description></description>
  	<servlet-class>blog.servlets.Article</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateArticle</servlet-name>
  	<display-name>UpdateArticle</display-name>
  	<description></description>
  	<servlet-class>blog.servlets.UpdateArticle</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Index</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddArticle</servlet-name>
  	<url-pattern>/AddArticle</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddAuteur</servlet-name>
  	<url-pattern>/AddAuteur</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListeArticle</servlet-name>
  	<url-pattern>/ListeArticle</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListeAuteur</servlet-name>
  	<url-pattern>/ListeAuteur</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Article</servlet-name>
  	<url-pattern>/Article</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateArticle</servlet-name>
  	<url-pattern>/UpdateArticle</url-pattern>
  </servlet-mapping>
  
  
</web-app>
